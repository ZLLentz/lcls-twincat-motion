<TcPlcObject Version="1.1.0.1">
  <POU Name="F_PosUnderUpperBound" Id="{b5ee37fa-13c6-40f2-a7c7-9ac80833d78f}" SpecialFunc="None">
    <Declaration><![CDATA[
FUNCTION F_PosUnderUpperBound : BOOL
VAR_INPUT
    fPosition: LREAL;
    stPositionState: ST_PositionState;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[
F_PosUnderUpperBound := fPosition < (stPositionState.fPosition + ABS(stPositionState.fDelta));
]]></ST>
    </Implementation>
  </POU>
